	FloatingStroke.Color = Theme.Primary
	FloatingStroke.Parent = FloatingButton
	
	FloatingButton.MouseButton1Click:Connect(function()
		TeddyUI_Premium.Enabled = not TeddyUI_Premium.Enabled
		
		Library:Notify({
			Title = TeddyUI_Premium.Enabled and "UI Opened" or "UI Closed",
			Description = TeddyUI_Premium.Enabled and "Welcome back!" or "Goodbye!",
			Type = TeddyUI_Premium.Enabled and "Success" or "Info",
			Duration = 2
		})
	end)
	
	Minize.MouseButton1Click:Connect(function()
		TeddyUI_Premium.Enabled = false
	end)
	
	return Tab
end

return Library
