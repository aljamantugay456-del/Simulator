TBtn.MouseButton1Click:Connect(function()
	vis=not vis
	local tgt=vis and UDim2.new(0.27,0,0.05,0) or UDim2.new(-0.6,0,0.05,0)
	TweenService:Create(i,TweenInfo.new(0.4,Enum.EasingStyle.Sine),{Position=tgt}):Play()
end)
