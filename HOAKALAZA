local at
ar.MouseButton1Click:Connect(function()
as=not as
ar.Text="RUN: "..(as and "ON" or "OFF")
if as then
at=task.spawn(function()
while as do
pcall(function()
local au=aq.Text
if au and #au>0 then
local av,aw=loadstring(au)
if av then pcall(av) end
end
end)
task.wait(0.01)
end
end)
else
if at then task.cancel(at) at=nil end
end
end)
end)
local ax=true
j.MouseButton1Click:Connect(function()
ax=not ax
d:Create(k,TweenInfo.new(0.4,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut),{Position=UDim2.new(0.5+(ax and 0 or -1.1),0,0.5,0)}):Play()
