local k=Instance.new("Frame",g)
k.Size=UDim2.new(0,480,0,320)
k.Position=UDim2.new(0.5,0,0.5,0)
k.AnchorPoint=Vector2.new(0.5,0.5)
k.BackgroundColor3=Color3.fromRGB(15,15,18)
k.BackgroundTransparency=0.35
Instance.new("UICorner",k).CornerRadius=UDim.new(0,14)
local l=Instance.new("UIStroke",k)
l.Color=Color3.fromRGB(255,255,255)
l.Thickness=1
l.Transparency=0.7
local m=Instance.new("TextLabel",k)
m.Size=UDim2.new(1,-20,0,35)
m.Position=UDim2.new(0,10,0,5)
m.BackgroundTransparency=1
m.Text="HOKALAZA HUB"
m.Font=Enum.Font.GothamBold
m.TextScaled=true
m.TextColor3=Color3.fromRGB(255,255,255)
m.TextStrokeTransparency=0.6
local n=false
local o,p
k.InputBegan:Connect(function(q)
if q.UserInputType==Enum.UserInputType.MouseButton1 then
n=true
o=q.Position
p=k.Position
end
end)
k.InputChanged:Connect(function(q)
if q.UserInputType==Enum.UserInputType.MouseMovement and n then
local r=q.Position-o
k.Position=UDim2.new(p.X.Scale,p.X.Offset+r.X,p.Y.Scale,p.Y.Offset+r.Y)
end
end)
k.InputEnded:Connect(function(q)
if q.UserInputType==Enum.UserInputType.MouseButton1 then
n=false
end
end)
local s=Instance.new("Frame",k)
s.Size=UDim2.new(0,120,1,-55)
s.Position=UDim2.new(0,10,0,45)
s.BackgroundTransparency=1
local t=Instance.new("UIListLayout",s)
t.SortOrder=Enum.SortOrder.LayoutOrder
t.Padding=UDim.new(0,6)
t.VerticalAlignment=Enum.VerticalAlignment.Top
local u=Instance.new("Folder",k)
u.Name="Pages"
local function v(w)
w.MouseEnter:Connect(function()
d:Create(w,TweenInfo.new(0.2),{BackgroundColor3=Color3.fromRGB(255,255,255),TextColor3=Color3.fromRGB(0,0,0)}):Play()
end)
w.MouseLeave:Connect(function()
d:Create(w,TweenInfo.new(0.2),{BackgroundColor3=Color3.fromRGB(220,220,220),TextColor3=Color3.fromRGB(20,20,20)}):Play()
end)
end
local function x(y)
local z=Instance.new("TextButton")
z.Size=UDim2.new(1,-10,0,35)
z.BackgroundColor3=Color3.fromRGB(220,220,220)
z.TextColor3=Color3.fromRGB(20,20,20)
z.Text=y
z.Font=Enum.Font.GothamBold
z.TextSize=14
Instance.new("UICorner",z).CornerRadius=UDim.new(0,8)
v(z)
return z
end
local function A(B,C)
local D
if C then
D=Instance.new("ScrollingFrame",u)
D.CanvasSize=UDim2.new(0,0,2,0)
D.ScrollBarThickness=4
D.ScrollBarImageColor3=Color3.fromRGB(255,255,255)
else
D=Instance.new("Frame",u)
end
D.Name=B
D.Size=UDim2.new(1,-140,1,-65)
D.Position=UDim2.new(0,130,0,45)
D.BackgroundColor3=Color3.fromRGB(255,255,255)
D.BackgroundTransparency=0.85
Instance.new("UICorner",D).CornerRadius=UDim.new(0,10)
local E=Instance.new("UIStroke",D)
E.Color=Color3.fromRGB(255,255,255)
E.Thickness=1
E.Transparency=0.7
D.Visible=false
return D
end
local F=A("AutoCoin")
local G=A("ExpFarm")
local H=A("Teleport",true)
local I=A("Run")
local J={
AutoCoin=x("Auto Coin"),
ExpFarm=x("Exp Farm"),
Teleport=x("Teleport"),
Run=x("Run Tab")
}
for _,K in pairs(J) do K.Parent=s end
local function L(M)
for _,N in pairs(u:GetChildren()) do
N.Visible=N.Name==M
end
end
J.AutoCoin.MouseButton1Click:Connect(function() L("AutoCoin") end)
J.ExpFarm.MouseButton1Click:Connect(function() L("ExpFarm") end)
J.Teleport.MouseButton1Click:Connect(function() L("Teleport") end)
J.Run.MouseButton1Click:Connect(function() L("Run") end)
L("AutoCoin")
local O=Instance.new("TextButton",F)
O.Size=UDim2.new(0,160,0,45)
O.Position=UDim2.new(0,30,0,40)
O.BackgroundColor3=Color3.fromRGB(255,255,255)
O.TextColor3=Color3.fromRGB(0,0,0)
O.Text="Auto Coin: OFF"
O.Font=Enum.Font.GothamBold
O.TextSize=16
Instance.new("UICorner",O).CornerRadius=UDim.new(0,8)
v(O)
local P=false
local Q
O.MouseButton1Click:Connect(function()
P=not P
O.Text="Auto Coin: "..(P and "ON" or "OFF")
if P then
Q=task.spawn(function()
local R=b:WaitForChild("Remotes"):WaitForChild("RecieveCoin")
while P do
pcall(function()
local S=c:FindFirstChild("Jeepnies")
if S then
for _,T in pairs(S:GetChildren()) do
local U=T:FindFirstChild("PassengerValues")
if U then
R:FireServer({Value=300,PassengerValues=U,Password=123456789})
end
end
end
end)
task.wait(0.02)
end
end)
else
if Q then task.cancel(Q) Q=nil end
end
end)
local V=Instance.new("TextButton",G)
V.Size=UDim2.new(0,160,0,45)
V.Position=UDim2.new(0,30,0,40)
V.BackgroundColor3=Color3.fromRGB(255,255,255)
V.TextColor3=Color3.fromRGB(0,0,0)
V.Text="Exp Farm: OFF"
V.Font=Enum.Font.GothamBold
V.TextSize=16
Instance.new("UICorner",V).CornerRadius=UDim.new(0,8)
v(V)
local W=false
local X
V.MouseButton1Click:Connect(function()
W=not W
V.Text="Exp Farm: "..(W and "ON" or "OFF")
if W then
X=task.spawn(function()
local Y=e:WaitForChild("leaderstats")
local Z=100
local aa=5000
while W do
local ab=Y:FindFirstChild("Exp")
local ac=Y:FindFirstChild("Cash")
if ab then ab.Value=ab.Value+Z end
if ac then ac.Value=ac.Value+aa end
task.wait(1)
end
end)
else
if X then task.cancel(X) X=nil end
end
end)
